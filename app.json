{
  "expo": {
    "name": "Ravi",
    "slug": "ravi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ravi-adapt.png",
        "backgroundColor": "#3C3C3C"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "An app to make align solar panels easy"
  }
}
